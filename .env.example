# 如果需要修改Docker暴露端口，请修改ports中的参数
SERVICE_PORT=3000

# 监听地址(非必填)
LISTEN_ADDRESS=0.0.0.0

# API 密钥配置（必填）
# 用于验证 API 请求
API_KEY=sk-123456

# 是否输出思考过程
OUTPUT_THINK=true

# 搜索信息显示模式
SEARCH_INFO_MODE=table

# 简化模型映射
# true: 只返回基础模型，不包含thinking、search、image等变体
# false: 返回完整模型列表，包含所有变体
SIMPLE_MODEL_MAP=false

# Qwen Cookie 配置（必填）
# 1. 直接复制完整的 Cookie 字符串即可
# 2. 多个账户之间用逗号(,)分隔
# 3. Cookie 包含很多字段，系统会自动提取需要的 token 字段
# 4. 系统会自动按顺序编号（第1个Cookie → account_1，第2个 → account_2，以此类推）
# 5. Cookie 会过期，需要定期手动更新
# 示例格式（多个账户用逗号分隔）：
# QWEN_COOKIES=token=xxx; ssxmod_itna=xxx,token=yyy; ssxmod_itna=yyy
#
QWEN_COOKIES=


# 日志配置
# 日志级别 (DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO

# 是否启用文件日志
ENABLE_FILE_LOG=false

# 日志文件目录
LOG_DIR=./logs

# 最大日志文件大小 (MB)
MAX_LOG_FILE_SIZE=10

# 保留的日志文件数量
MAX_LOG_FILES=5